{
  "name": "fuse19-backend",
  "version": "1.0.0",
  "description": "Node.js backend for Fuse Angular 19 template",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "seed": "node seeders/index.js",
    "seed:clear": "node seeders/index.js --clear-all",
    "seed:users": "node seeders/index.js --users-only",
    "seed:contacts": "node seeders/index.js --contacts-only",
    "seed:notes": "node seeders/index.js --notes-only",
    "seed:tasks": "node seeders/index.js --tasks-only",
    "seed:prod": "NODE_ENV=production node seeders/index.js --yes",
    "process-filings": "node scripts/process-filings.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.4",
    "nodemailer": "^7.0.5",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.2",
    "playwright": "^1.54.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "fuse",
    "angular",
    "backend",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT"
}
